{
    "name": "lanette/lag-drup1",
    "description": "Project template for Drupal projects made from lagoonexamples/drupalnext-base with MariaDB, hosted on Lagoon",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "The Lagoon Authors",
            "homepage": "https://github.com/uselagoon",
            "email": "uselagoon@amazee.io"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
        "type": "package",
        "package": {
            "name": "krakjoe/apcu",
            "version": "5.1.23",
            "source": {
                "url": "https://github.com/krakjoe/apcu.git",
                "type": "git",
                "reference" : "origin/master"
            }
        }
    }
    ],
    "require": {
        "amazeeio/drupal_integrations": "0.5.0",
        "composer/installers": "2.3.0",
        "cweagans/composer-patches": "1.7.3",
        "drupal/apcu_diagnostics": "^1.0",
        "drupal/core-composer-scaffold": "^11.2",
        "drupal/core-project-message": "^11.2",
        "drupal/core-recommended": "^11.2",
        "drupal/fastly": "^4.0",
        "drupal/http_cache_control": "^2.6",
        "drupal/lagoon_logs": "3.0.1",
        "drupal/mysql57": "1.0.0",
        "drupal/purge": "^3.6",
        "drupal/reverse_proxy_header": "^1.1",
        "drupal/ultimate_cron": "^2.0@beta",
        "drush/drush": "^13.7",
        "mglaman/composer-drupal-lenient": "1.0.7"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config":{
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "mglaman/composer-drupal-lenient": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "amazeeio/drupal_integrations"
            ],
            "file-mapping": {
                "[web-root]/sites/default/all.settings.php": "assets/all.settings.php"
            },
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
